@(Html.Kendo().Window()
    .Name("window")
    .Title("About Josef Hoffmann")
    .Actions(actions => actions
        .Custom("custom")
        .Minimize()
        .Maximize()
        .Close()
    )
    .Content("Loading form...")
    .LoadContentFrom("Create", "Book")
    .Draggable()
    .Resizable()
    .Events(ev => ev.Close("onClose"))
    .Width(500)
)
